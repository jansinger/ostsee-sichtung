{
	"name": "ostsee-tiere",
	"private": true,
	"version": "1.0.0",
	"description": "Ostsee-Tiere - Marine animal sighting reporting platform for the Baltic Sea",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint . --config eslint.config.js",
		"lint:fix": "eslint . --fix --config eslint.config.js",
		"lint:debug": "eslint . --config eslint.config.js --debug",
		"audit": "npm audit --audit-level=moderate",
		"type-check": "tsc --noEmit",
		"test:unit": "vitest",
		"test:unit:watch": "vitest --watch",
		"test": "npm run test:quick",
		"test:quick": "npm run lint && npm run test:unit -- --run && npm run test:e2e",
		"test:production": "npm run type-check:production && npm run lint && npm run test:unit -- --run && npm run test:e2e",
		"type-check:production": "tsc --noEmit --project tsconfig.production.json",
		"test:e2e": "playwright test",
		"db:start": "docker compose up -d",
		"db:stop": "docker compose down",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.18.0",
		"@fontsource/inter": "^5.2.6",
		"@fontsource/roboto": "^5.2.6",
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-vercel": "^5.8.2",
		"@sveltejs/kit": "^2.30.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@types/node": "^22.17.1",
		"@vercel/blob": "^1.1.1",
		"@vitest/browser": "^3.2.3",
		"daisyui": "^5.0.50",
		"drizzle-kit": "^0.31.4",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"pino": "^9.9.0",
		"pino-pretty": "^13.1.1",
		"playwright": "^1.53.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.1",
		"svelte-check": "^4.3.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.1.2",
		"vite-plugin-devtools-json": "^0.2.1",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"@steeze-ui/lucide-icons": "^1.4.1",
		"@steeze-ui/svelte-icon": "^1.6.2",
		"@turf/boolean-point-in-polygon": "^7.2.0",
		"@turf/helpers": "^7.2.0",
		"@vitejs/plugin-basic-ssl": "^2.1.0",
		"drizzle-orm": "^0.44.4",
		"exifr": "^7.1.3",
		"flowbite-svelte-icons": "^2.2.1",
		"geojson-rbush": "^3.2.0",
		"jsonwebtoken": "^9.0.2",
		"jwks-rsa": "^3.2.0",
		"ol": "^10.6.1",
		"postgres": "^3.4.7",
		"rbush": "^4.0.1",
		"svelte-forms-lib": "^2.0.1",
		"tailwind-variants": "^2.1.0",
		"yup": "^1.6.1"
	}
}
